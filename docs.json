{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Aethermark",
  "colors": {
    "primary": "#229799",
    "light": "#229799",
    "dark": "#229799"
  },
  "icons": {
    "library": "lucide"
  },
  "interaction": {
    "drilldown": false
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "quickstart", "development"]
          },
          {
            "group": "Customization",
            "pages": ["essentials/settings", "essentials/navigation"]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://aethermark.dev"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/aethermark"
    }
  },
  "seo": {
    "metatags": {
      "canonical": "https://aethermark.dev",
      "og:locale": "en_US"
    }
  },
  "fonts": {
    "heading": {
      "family": "Space Grotesk",
      "source": "https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@400;500&display=swap",
      "format": "woff2"
    },
    "body": {
      "family": "Noto Sans",
      "source": "https://fonts.googleapis.com/css2?family=Noto+Sans:wght@400;500&display=swap",
      "format": "woff2"
    }
  }
}
